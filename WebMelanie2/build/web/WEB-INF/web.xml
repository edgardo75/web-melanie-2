<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ShowReportBarrios</servlet-name>
        <servlet-class>com.server.ShowReportBarrios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowReportEntreFechasEntrega</servlet-name>
        <servlet-class>com.server.ShowReportEntreFechasEntrega</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowReportNotaPedido</servlet-name>
        <servlet-class>com.server.ShowReportNotaPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowReportPresupuesto</servlet-name>
        <servlet-class>com.server.ShowReportPresupuesto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowReportBarrios</servlet-name>
        <url-pattern>/ShowReportBarrios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowReportEntreFechasEntrega</servlet-name>
        <url-pattern>/ShowReportEntreFechasEntrega</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowReportNotaPedido</servlet-name>
        <url-pattern>/ShowReportNotaPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowReportPresupuesto</servlet-name>
        <url-pattern>/ShowReportPresupuesto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MelaniSuvenir.html</welcome-file>
    </welcome-file-list>
</web-app>
